{
  "manifest_version": 2,

  "name": "Anti-Membean Weapon: Juno",
  "description": "Provides features to help students complete their membean.com homework!",
  "version": "2.0.1",
  "page_action": {
    "default_icon": "favicon.png",
    "default_popup": "popup.html",
    "default_title": "Get Started"
  },
  "permissions": [
     "activeTab"
  ],
  "content_scripts": [
    {
        "matches": ["http://*/*", "https://*/*"],
        "js": ["jquery.js", "content.js"]
    }
]
}
